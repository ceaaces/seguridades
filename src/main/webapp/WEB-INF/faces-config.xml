<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd"
	version="2.0">

	<application>
		<el-resolver>org.springframework.web.jsf.el.SpringBeanFacesELResolver</el-resolver>
	</application>

	<navigation-rule>
		<from-view-id>/opApp.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>seguridadApp</from-outcome>
			<to-view-id>/seguridadApp.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/ingreso.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>inSeguridades</from-outcome>
			<to-view-id>/templates/template.xhtml</to-view-id>			
		</navigation-case>
	</navigation-rule>

	<navigation-rule>
		<from-view-id>/templates/template.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>outSeguridades</from-outcome>
			<to-view-id>/ingreso.xhtml</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>
	
	<navigation-rule>
		<from-view-id>/seguridadApp.xhtml</from-view-id>
		<navigation-case>
			<from-outcome>outSeguridades</from-outcome>
